{
  "version": 2,
  "name": "bernard-lusale-portfolio",
  "builds": [
    {
      "src": "src/Portfolio.Web/Portfolio.Web.csproj",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "src/Portfolio.Web/bin/Release/net9.0/publish/wwwroot"
      }
    }
  ],
  "buildCommand": "dotnet publish src/Portfolio.Web/Portfolio.Web.csproj -c Release -o src/Portfolio.Web/bin/Release/net9.0/publish",
  "outputDirectory": "src/Portfolio.Web/bin/Release/net9.0/publish/wwwroot",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}